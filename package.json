{
  "name": "fsjsd-lgr",
  "version": "0.0.7",
  "description": "magical javascript logger",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "cross-env npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=\"development\" rollup -c \"./scripts/rollup.config.js\"",
    "build:prod": "cross-env NODE_ENV=\"production\" rollup -c \"./scripts/rollup.config.js\"",
    "demo": "cross-env NODE_ENV=\"demo\" rollup -c \"./scripts/rollup.config.js\" && live-server --open=./demo/ ",
    "test": "jest",
    "testdev": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fsjsd/lgr.git"
  },
  "bugs": {
    "url": "https://github.com/fsjsd/lgr/issues"
  },
  "homepage": "https://github.com/fsjsd/lgr#readme",
  "author": "Chris Webb",
  "license": "MIT",
  "keywords": [
    "console",
    "logger",
    "logging",
    "react",
    "javascript"
  ],
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-jest": "24.8.0",
    "cross-env": "5.2.0",
    "enzyme": "3.10.0",
    "jest": "24.8.0",
    "live-server": "^1.2.1",
    "prettier": "1.18.2",
    "rollup": "1.15.5",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.2",
    "rollup-plugin-replace": "2.2.0"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  }
}
